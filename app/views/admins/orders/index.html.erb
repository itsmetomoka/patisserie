<div class="container">

<!-- タイトル -->
<h2 class="m-4">注文履歴一覧</h2>

	<!-- 一覧 -->
	<table class="table">
		<thead>
			<th>購入日時</th>
			<th>購入者</th>
			<th>注文個数</th>
			<th>注文ステータス</th>
		</thead>
		<% @orders.each do |history| %>
			<tbody>
				<td><%= link_to history.created_at, admins_order_path(history)%></td>
				<td><%= history.customer.first_name %></td>
				<td><%=  %></td>
				<td><%= history.order_status %></td>
			</tbody>
		<% end %>
	</table>
</div>