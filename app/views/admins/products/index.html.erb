
<div class="container">

<!-- タイトル -->
<h2 class="m-4">商品一覧</h2>

<!-- 一覧 -->
<table class="table">
	<thead>
		<th>商品ID</th>
		<th>商品名</th>
		<th>税抜価格</th>
		<th>ジャンル</th>
		<th>ステータス</th>
	</thead>
	<% @products.each do |item| %>
		<tbody>
			<td><%= item.id %></td>
			<td><%= item.name %></td>
			<td><%= item.price %></td>
			<td><%= item.category %></td>
			<td><%= item.is_active %></td>
		</tbody>
	<% end %>
</table>


</div>